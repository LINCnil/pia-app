{
  "name": "PIA",
  "version": "1.0.0",
  "description": "PIA application",
  "main": "main.js",
  "author": "Atnos",
  "scripts": {
    "package-mac": "electron-packager . --electron-version='1.6.11' --asar=true --overwrite --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --electron-version='1.6.11' --asar=true --overwrite --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=ATNOS --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . --electron-version='1.6.11' --asar=true --overwrite --platform=linux --arch=x64 --icon=icons/png/1024x1024.png --prune=true --out=release-builds"
  }
}
